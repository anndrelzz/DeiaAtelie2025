{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "admin.html", "use": "@vercel/static" },
    { "src": "deia_admin.html", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "css/$1"
    },
    {
      "src": "/admin.html",
      "dest": "admin.html"
    },
    {
      "src": "/deia_admin.html",
      "dest": "deia_admin.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}